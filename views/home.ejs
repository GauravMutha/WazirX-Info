<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>MY APP</title>
    <link rel="stylesheet" href="/home.css">
    <link rel="stylesheet" href="/header.css">
    <link rel="stylesheet" href="/footer.css">
    <script src="./public/main.js" defer></script>
</head>

<body>
    <%- include('_header') %>
        <div class="middle-container">
            <!-- <div class="loader_page"></div> -->
            <div class="table-container">
                <table class="crypto-table">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Name</th>
                            <th>Last</th>
                            <th>Buy</th>
                            <th>Sell</th>
                            <th>Voulme</th>
                            <th>Base Unit</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%let i=0%>
                        <% dataFromDB.forEach(row => { %>
                            <%i++%>
                          <tr class="dataRows">
                            <td><%= i%></td>
                            <td><%= row.name %></td>
                            <td><%= row.last %></td>
                            <td><%= row.buy %></td>
                            <td><%= row.sell %></td>
                            <td><%= row.volume %></td>
                            <td><%= row.base_unit %></td>
                          </tr>
                        <% }); %>
                    </tbody>
                </table>
            </div>
        </div>
        <%- include('_footer') %>
</body>

</html>
